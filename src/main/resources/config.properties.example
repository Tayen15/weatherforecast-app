# PostgreSQL Database Configuration
db.host=localhost
db.port=5432
db.name=weatherdb
db.username=your_database_username
db.password=your_database_password
db.url=jdbc:postgresql://${db.host}:${db.port}/${db.name}

# OpenWeatherMap API Configuration
# Get your free API key from: https://openweathermap.org/api
weather.api.key=your_openweathermap_api_key_here
weather.api.url=https://api.openweathermap.org/data/2.5/weather

# INSTRUCTIONS:
# 1. Copy this file and rename it to: config.properties
# 2. Replace all placeholder values with your actual credentials
# 3. NEVER commit the actual config.properties file to version control
